        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Setting_Change_Password" Height="208" Width="300" FlowDirection="RightToLeft" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded">
    <Grid>
        <ComboBox Height="23" HorizontalAlignment="Left" ItemsSource="{Binding}" Margin="88,10,0,0" Name="comboBox1" TabIndex="0" VerticalAlignment="Top" Width="176" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="88,39,0,0" Name="passwordBox1" TabIndex="1" VerticalAlignment="Top" Width="176" />
        <Label Content="نام کاربر" Height="23" HorizontalAlignment="Left" Margin="10,10,0,0" Name="label1" VerticalAlignment="Top" Width="72" />
        <Label Content="رمز ورود قبلی" Height="23" HorizontalAlignment="Left" Margin="10,39,0,0" Name="label2" VerticalAlignment="Top" Width="72" />
        <Button Content="تغییر رمز" Height="23" HorizontalAlignment="Left" Margin="10,135,0,0" Name="button1" TabIndex="2" VerticalAlignment="Top" Width="254" Click="button1_Click" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="88,68,0,0" Name="passwordBox2" TabIndex="1" VerticalAlignment="Top" Width="176" />
        <Label Content="رمز ورود جدید" Height="23" HorizontalAlignment="Left" Margin="10,68,0,0" Name="label3" VerticalAlignment="Top" Width="72" />
    </Grid>
</Window>
